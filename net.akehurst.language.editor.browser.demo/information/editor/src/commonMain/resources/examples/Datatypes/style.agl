namespace net.akehurst.language.example
styles Datatypes {
    'primitive', 'collection', 'class' {
      foreground: purple;
      font-style: bold;
    }
    ID {
      foreground: blue;
      font-style: italic;
    }
    '{', '}' {
      foreground: darkgreen;
      font-style: bold;
    }
    property {
      background: lightgray;
    }
    typeReference {
      foreground: green;
      background: lightblue;
    }
}